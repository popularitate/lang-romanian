{
	"name": "popularitate/lang-romanian",
	"type": "flarum-extension",
	"description": "Romanian language pack for Flarum.",
	"homepage": "https://github.com/popularitate/lang-romanian",
	"keywords": [
		"locale",
		"flarum",
		"romanian-locale"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Breaker",
			"homepage": "https://www.apexlegends.ro"
		}
	],
